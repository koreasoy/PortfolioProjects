ods noproctitle;

/*** Analyze categorical variables ***/
title "Frequencies for Categorical Variables";

proc freq data=FPP23.CORCOMPDATA;
	tables Restaurant_ID Items_Ordered / plots=(freqplot);
run;

/*** Analyze numeric variables ***/
title "Descriptive Statistics for Numeric Variables";

proc means data=FPP23.CORCOMPDATA n nmiss min mean median max std;
	var Time_Delivered ' Item_Price'n;
run;

title;

proc univariate data=FPP23.CORCOMPDATA noprint;
	histogram Time_Delivered ' Item_Price'n;
run;

/*** Analyze date variables ***/
title "Minimum and Maximum Dates";

proc sql;
	select "Date_Delivered" label="Date variable", min(Date_Delivered) 
		format=MMDDYY10. label="Minimum date" , max(Date_Delivered) 
		format=MMDDYY10. label="Maximum date" from FPP23.CORCOMPDATA;
quit;
